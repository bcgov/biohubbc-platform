{
  "name": "biohub-app",
  "version": "0.0.0",
  "description": "BioHub Web App",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcgov/biohubbc-platform.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set TSC_COMPILE_ON_ERROR=true && set ESLINT_NO_DEV_ERRORS=true && react-scripts build",
    "deploy_start": "node server",
    "test": "react-scripts test --env=jsdom-fourteen --ci --watchAll=false --runInBand",
    "test:watch": "react-scripts test --env=jsdom-fourteen",
    "coverage": "react-scripts test --ci --coverage --testResultsProcessor jest-sonar-reporter --env=jsdom-fourteen --watchAll=false --runInBand",
    "update-snapshots": "react-scripts test --env=jsdom-fourteen --ci --watchAll=false --updateSnapshot",
    "lint": "eslint src/ --ext .jsx,.js,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "format-fix": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss}\""
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "dependencies": {
    "@bcgov/bc-sans": "~1.0.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mdi/js": "~6.4.95",
    "@mdi/react": "~1.4.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.94",
    "@mui/material": "^5.10.0",
    "@mui/styles": "^5.9.3",
    "@react-keycloak/web": "^3.4.0",
    "@react-leaflet/core": "~1.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@tmcw/togeojson": "~4.2.0",
    "@turf/bbox": "~6.3.0",
    "@turf/boolean-equal": "~6.3.0",
    "@turf/intersect": "~6.3.0",
    "axios": "~0.21.4",
    "clsx": "~1.1.1",
    "express": "~4.17.1",
    "formik": "~2.2.6",
    "keycloak-js": "^19.0.1",
    "leaflet": "~1.7.1",
    "leaflet-draw": "~1.0.4",
    "leaflet-fullscreen": "~1.0.2",
    "leaflet.locatecontrol": "~0.76.0",
    "lodash-es": "~4.17.21",
    "moment": "~2.29.2",
    "node-sass": "~4.14.1",
    "qs": "~6.9.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "~11.3.2",
    "react-leaflet": "~3.1.0",
    "react-leaflet-cluster": "~1.0.3",
    "react-number-format": "~4.5.2",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "react-window": "~1.8.6",
    "reproj-helper": "~1.2.8",
    "shpjs": "^3.6.3",
    "typescript": "~4.1.6",
    "uuid": "~8.3.2",
    "yup": "~0.32.9"
  },
  "devDependencies": {
    "@babel/preset-typescript": "~7.12.7",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "~12.6.0",
    "@types/geojson": "~7946.0.7",
    "@types/jest": "~26.0.20",
    "@types/leaflet": "~1.7.9",
    "@types/leaflet-draw": "~1.0.5",
    "@types/leaflet-fullscreen": "~1.0.6",
    "@types/lodash-es": "~4.17.4",
    "@types/node": "~14.14.31",
    "@types/node-sass": "~4.11.2",
    "@types/qs": "~6.9.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-leaflet": "~2.8.2",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "~1.8.2",
    "@types/shpjs": "^3.4.0",
    "@types/uuid": "~8.3.0",
    "@typescript-eslint/eslint-plugin": "~3.10.1",
    "@typescript-eslint/parser": "~3.10.1",
    "axios-mock-adapter": "~1.19.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-plugin-prettier": "~3.3.1",
    "jest": "^24.9.0",
    "jest-sonar-reporter": "~2.0.0",
    "prettier": "~2.3.2",
    "prettier-plugin-organize-imports": "~2.3.4",
    "react-scripts": "^3.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/src/themes/**",
      "!<rootDir>/src/constants/**",
      "!<rootDir>/src/App.tsx",
      "!<rootDir>/src/types/**",
      "!<rootDir>/src/interfaces/**",
      "!<rootDir>/src/AppRouter.tsx",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/node_modules/**",
      "!<rootDir>/coverage/**",
      "!<rootDir>/public/**",
      "!<rootDir>/build/**",
      "!<rootDir>/src/serviceWorker.**",
      "!<rootDir>/src/setupTests.*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
